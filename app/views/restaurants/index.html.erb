<div class="container">
  <div class="col-xs-12 col-md-4"></div>
  <div class="col-xs-12 col-md-4">

    <div class="text-left">
      <h2>Restaurants</h2>
      <% count = 1 %>
      <% @restaurants.each do |restaurant| %>
        <h3><%= count %> <%= link_to restaurant.name, restaurant_path(restaurant) %> </h3>

        <% count += 1 %>
      <% end %>
    </div>
  </div>
  <div class="col-xs-12 col-md-4"></div>

</div>
